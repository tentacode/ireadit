{
    "name": "ireadit - Organisez votre veille technique",
    "description": "Organisez votre veille technique !",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "default_icon": {
      "16": "/img/get_started16.png",
      "32": "/img/get_started32.png",
      "48": "/img/get_started48.png",
      "128": "/img/get_started128.png"
    },
    "icons": {
      "16": "/img/get_started16.png",
      "32": "/img/get_started32.png",
      "48": "/img/get_started48.png",
      "128": "/img/get_started128.png"
    }
}